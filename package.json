    "jquery": "^3.2.1",
    "materialize-css": "^0.100.2",
    "musicmetadata": "^2.0.5"
  },
  "devDependencies": {
    "@types/jquery": "^3.2.12",
    "@types/node": "^7.0.13",
    "electron": "^1.7.6",
    "electron-builder": "^19.27.3",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.1.6",
    "ts-node": "^2.1.0",
    "typescript": "^2.2.1"
