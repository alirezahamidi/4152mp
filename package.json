{
  "name": "4152mp",
  "version": "1.0.0",
  "description": "4152Production Music Player",
  "main": "source/main.js",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "Music",
    "Player",
    "4152mp"
